{
    "JavaCLI": {
        "description": "Get data from website in specific region of world",
        "command": "java -jar ~/SampleCLI.jar",
        "args-order": ["src", "region", "startTime", "binary", "partition"],
        "args": {
            "src": {
                "type": "string",
                "values": ["www.google.com", "www.apple.com"],
                "default": "www.yahoo.com"
            },
            "region": {
                "type": "string",
                "values": ["US", "EU", "ASIA"],
                "default": "US"
            },
            "startTime": {
                "type": "string",
                "values": "(0,5,1) -> strftime(\"%Y-%m-%d\", localtime() - v:val*24*60*60)"
            },
            "binary": {
                "type": "boolean",
                "default": "_enabled_"
            },
            "partition": {
                "values": "(0,11,1) -> v:val"
            }
        },
        "parser": {
            "src": "s: (\\S+) ",
            "region": "r: (\\w+) ",
            "partition": "p: (\\d+) "
        }
    },
    "StockReader": {
        "description": "Ouput statistic for a stock",
        "command": "python  ~/stock_reader.py",
        "args-order": ["exchange", "ticker", "date", "statistic"],
        "args": {
            "exchange": {
                "values": ["NYSE", "NASDAQ", "JPX", "XLON"],
                "default": "NYSE"
            },
            "ticker": {
                "values": ["AAPL", "PLTR"],
                "default": "AAPL"
            },
            "date": {
                "values": ["2020-04-01"]
            },
            "statistic": {
                "values": ["High", "Low", "Avg", "Volume"]
            }
        }

    }
}
